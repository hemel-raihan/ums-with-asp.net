@model IEnumerable<UniversityManagementSystem.Models.DataAccess.student>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Upload File</title>
    <script type="text/javascript" src="{{asset('jquery-3.5.1.min.js')}}"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>

    <link rel="stylesheet" href="~/Content/Css/fileupload.css">
</head>
<body>
    <div class="content">


        <div class="form">

            <h4>Add File</h4>
            <form method="post" enctype="multipart/form-data">

                <font color="green"> <p> Photo Title :</p><input type="text" name="title"></font>


                <font color="green"> <p> Upload Photo :</p><input type="file" name="photo"></font>

                <font color="green"> <p> photo description :</p><input type="text" name="description"></font>
              
                    <font color="green"> <p>  :</p><input type="text" name="uid" value="@ViewData["id"]"></font>
                
                 
                    <input type="submit" value="upload">
                
            </form>
                
            <a class="btn btn-primary" href="/dashboard/index" role="button">Back</a></br>

        </div>

    </div>




</body>
</html>